{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "configurationContext":{
    "dbprovider": "SQL_SERVER",
    "dbConnection": "Data Source=localhost;database=Hydra.IdentityServer.Configuration; User Id=<user_id>;Password=<password>;",
    "seedDb": true,
    "runMigration": false
  },
  "applicationContext":{
    "dbprovider": "SQL_SERVER",
    "dbConnection": "Data Source=localhost;database=Hydra.IdentityServer.Application; User Id=<user_id>;Password=<password>;",
    "seedDb": true,
    "runMigration": false
  },
  "googleAuthentication":{
    "clientId": "<client_id>",
    "clientSecret": "<secret_id>"
  },
  "cookieExpiration":
    {
      "hour": 1,
      "minutes": 30
    }
}